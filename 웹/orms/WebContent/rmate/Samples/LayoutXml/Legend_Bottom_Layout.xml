		<Line2DSeries yField="Data7"  radius="5" fill="0xEFEFEF" displayName="Data7" itemRenderer="RectangleItemRenderer">
			<stroke>
				<Stroke color="0x86D1E4 " weight="1"/>
			</stroke>
			<lineStroke>
				<Stroke color="0x86D1E4 " weight="2"/>
			</lineStroke>
			<showDataEffect>
		        <SeriesInterpolate/>
			</showDataEffect>
		</Line2DSeries>	
    </series>
</Line2DChart>
</rMateChart>
