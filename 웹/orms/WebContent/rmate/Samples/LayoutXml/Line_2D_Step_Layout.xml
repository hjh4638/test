<?xml version="1.0" encoding="utf-8"?>
<rMateChart backgroundColor="0xFFFFFF"  cornerRadius="12" borderStyle="solid">
	<Options>
		<Caption text="Anual Report"/>
	</Options>
	<Line2DChart showDataTips="true" >
		<horizontalAxis>
			<CategoryAxis categoryField="Month" padding="0.5"/> 
		</horizontalAxis>
		<series>
			<Line2DSeries labelPosition="up" yField="Profit" form="step" itemRenderer="CircleItemRenderer">
			<!-- form속성으로는 step입니다 -->
				<showDataEffect>
					<SeriesInterpolate/> 
				</showDataEffect>
			</Line2DSeries>
		</series>
		<annotationElements>
			<CanvasElement>
				<Label width="100" height="40" x="10" y="10" text="★주목!&#xa;메모 기능입니다." fontSize="12" textAlign="center" borderStyle="solid" borderColor="0xeca614" color="0x000000" backgroundColor="0xeca614" backgroundAlpha=".7">
					<filters>
						<DropShadowFilter/>
					</filters>
				</Label>
				<Label x="400" y="250" text="By RiaMore." fontSize="12" textAlign="center" borderStyle="solid" borderColor="0xeca614" color="0x000000" backgroundColor="0xeca614" backgroundAlpha=".3"/>
			</CanvasElement>
		</annotationElements>
	</Line2DChart>
</rMateChart>