<?xml version="1.0" encoding="utf-8"?>
<rMateChart backgroundColor="0xFFFFFF"  cornerRadius="12" borderStyle="solid">
<Column3DChart  showDataTips="true" >
        <horizontalAxis>
           <CategoryAxis categoryField="Month"/>
        </horizontalAxis>
       <series>
			<Column3DSeries yField="Profit">
				<fill>
				<!-- Series안에 색을 채울 때(단일색) fill에 주목 -->
					<SolidColor color="0xf8abab" alpha="1"/>
					<!-- color:색깔 alpha:투명도 -->
				</fill>
				<stroke>
				<!-- Series의 외곽에 선을 넣을 때 -->
					<Stroke weight="1" color="0xf8abab"/>
					<!-- color:색깔 wegight:굵기 -->
				</stroke>
				<showDataEffect>
  					<SeriesInterpolate/> 
  				</showDataEffect>
			</Column3DSeries>
		</series>
     </Column3DChart>

</rMateChart>